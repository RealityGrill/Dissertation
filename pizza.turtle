#################################################################
#
#    Annotation properties
#
#################################################################


###  http://www.ncl.ac.uk/pizza#creator

:creator rdf:type owl:AnnotationProperty ;
         
         <http://www.purl.org/ontolink/tawny#name> "creator"@en ;
         
         rdfs:subPropertyOf rdfs:comment .



###  http://www.ncl.ac.uk/pizza#myOpinion

:myOpinion rdf:type owl:AnnotationProperty ;
           
           rdfs:label "My Opinion"@en ;
           
           rdfs:comment "Do I think this is a good pizza to eat?"@en ;
           
           :creator "Phillip Lord"@en ;
           
           <http://www.purl.org/ontolink/tawny#name> "myOpinion"@en ;
           
           rdfs:subPropertyOf rdfs:comment .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.ncl.ac.uk/pizza#hasBase

:hasBase rdf:type owl:FunctionalProperty ,
                  owl:ObjectProperty ;
         
         rdfs:label "con Baso "@it ;
         
         <http://www.purl.org/ontolink/tawny#name> "hasBase"@en ;
         
         rdfs:domain :Pizza ;
         
         rdfs:range :PizzaBase ;
         
         rdfs:subPropertyOf :hasIngredient .



###  http://www.ncl.ac.uk/pizza#hasIngredient

:hasIngredient rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               
               rdfs:label "con Ingredienti"@it ;
               
               :creator "Phillip Lord"@en ;
               
               <http://www.purl.org/ontolink/tawny#name> "hasIngredient"@en ;
               
               owl:inverseOf :isIngredientOf .



###  http://www.ncl.ac.uk/pizza#hasSpiciness

:hasSpiciness rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:label "con Piccantezza"@it ;
              
              :creator "Phillip Lord"@en ;
              
              <http://www.purl.org/ontolink/tawny#name> "hasSpiciness"@en .



###  http://www.ncl.ac.uk/pizza#hasTopping

:hasTopping rdf:type owl:ObjectProperty ;
            
            rdfs:label "con Ingredienti"@it ;
            
            <http://www.purl.org/ontolink/tawny#name> "hasTopping"@en ;
            
            rdfs:domain :Pizza ;
            
            rdfs:range :PizzaTopping ;
            
            rdfs:subPropertyOf :hasIngredient .



###  http://www.ncl.ac.uk/pizza#isIngredientOf

:isIngredientOf rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                
                rdfs:label "Ã¨ Ingredienti di"@it ;
                
                <http://www.purl.org/ontolink/tawny#name> "isIngredientOf"@en .





