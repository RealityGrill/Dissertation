###  http://www.ncl.ac.uk/pizza#CajunPizza

:CajunPizza rdf:type owl:Class ;
            
            rdfs:label "Cajun Pizza"@it ;
            
            rdfs:subClassOf :NamedPizza ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:someValuesFrom :PeperonataTopping
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:someValuesFrom :TobascoPepperSauce
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:someValuesFrom :MozzarellaTopping
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:unionOf ( :MozzarellaTopping
                                                                :OnionTopping
                                                                :PeperonataTopping
                                                                :PrawnsTopping
                                                                :TobascoPepperSauce
                                                                :TomatoTopping
                                                              )
                                                ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:someValuesFrom :OnionTopping
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:someValuesFrom :PrawnsTopping
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasTopping ;
                              owl:someValuesFrom :TomatoTopping
                            ] ;
            
            <http://www.purl.org/ontolink/tawny#name> "CajunPizza"@en .



